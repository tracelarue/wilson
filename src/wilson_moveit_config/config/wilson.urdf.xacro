<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="wilson">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import wilson urdf file -->
    <xacro:include filename="$(find wilson)/urdf/wilson_moveit.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="wilson.ros2_control.xacro" />


    <xacro:wilson_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
