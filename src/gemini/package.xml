<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>gemini</name>
  <version>0.0.0</version>
  <description>ROS2 package for Gemini AI integration</description>
  <maintainer email="traceglarue@gmail.com">trace</maintainer>
  <license>TODO: License declaration</license>

  <depend>rclpy</depend>
  <depend>action_msgs</depend>
  <depend>nav2_msgs</depend>
  <depend>geometry_msgs</depend>
  <depend>tf_transformations</depend>
  <depend>python3-pip</depend>
  <depend>google-generativeai</depend>
  <depend>python3-opencv</depend>
  <depend>python3-pyaudio</depend>
  <depend>python3-pillow</depend>
  <depend>python3-mss</depend>

  <test_depend>ament_copyright</test_depend>
  <test_depend>ament_flake8</test_depend>
  <test_depend>ament_pep257</test_depend>
  <test_depend>python3-pytest</test_depend>

  <export>
    <build_type>ament_python</build_type>
  </export>
</package>
